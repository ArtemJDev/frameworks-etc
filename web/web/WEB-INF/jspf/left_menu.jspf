<%@page import="main.java.beans.Genre"%>
<%@ page pageEncoding="UTF-8" %>
<div class="factory">
    <h4>Детали:</h4>
    <ul class="nav">
        <jsp:useBean id="genreList" class="main.java.beans.GenreList" scope="application"/>
        <br>
        <li><a href="books.jsp?genre_id=0">Все детали</a></li>
        <%
            for (Genre genre : genreList.getGenreList()) {
        %>
        <li><a href="books.jsp?genre_id=<%=genre.getId()%>&name=<%=genre.getName()%>"><%=genre.getName()%></a></li>

        <%}%>
    </ul>
</div>
