<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>

  <bean id="ToshibaHand1" class="ru.javabegin.training.spring.impls.toshiba.ToshibaHand" >
    <property name="color" value="Golden"/>
    <qualifier value="sonyGoldenHand"></qualifier>
    </bean>

  <bean id="ToshibaHand2" class="ru.javabegin.training.spring.impls.toshiba.ToshibaHand" >
    <property name="color" value="Bronze"/>
    <qualifier value="sonyBronzeHand"></qualifier>
    </bean>

  <bean id="ToshibaHead" class="ru.javabegin.training.spring.impls.toshiba.ToshibaHead"/>
  <bean id="ToshibaLeg" class="ru.javabegin.training.spring.impls.toshiba.ToshibaLeg"/>

</beans>